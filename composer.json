{
    "name": "kotamat/laravel-apispec-generator",
    "description": "RestAPI spec generator with Laravel test",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "kotamat",
            "email": "kota.matsumoto@scouter.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "ApiSpec\\": "src/",
            "Test\\ApiSpec\\": "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApiSpec\\ApiSpecServiceProvider"
            ]
        }
    },
    "require": {
        "laravel/framework": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.2"
    }
}
